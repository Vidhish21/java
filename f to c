import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    float temp;
	    Scanner sc = new Scanner(System.in);
		System.out.println("Enter tempertaure in fahrenheit: ");
		temp=sc.nextInt();
		temp = ((temp-32) * 5)/9;
		System.out.println("temperature in celsius = " + temp);
	}
}
