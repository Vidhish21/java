import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int n,c;
	    System.out.println("enter number: ");
	     System.out.println("enter value upto which you want the table: ");
	    Scanner sc = new Scanner(System.in);
	    n=sc.nextInt();
	    c=sc.nextInt();
	    for(int i=0;i<=c;i++){
	        System.out.println("the multiplication is " + (n*i));
	    }
	}
}
