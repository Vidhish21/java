import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    String st;
	    int a,b;
	   System.out.println("Enter the string: ");
	   st=sc.next();
	   b=st.length();
	   for(a=0;a<b;a++){
	       System.out.println(st.charAt(a));
	   }
	}
	}
