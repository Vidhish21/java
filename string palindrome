import java.util.*;
public class Main{
    public static void main(String args[])
    {
        String word,word1="";
        int i,p;
        char ch;
        Scanner sc=new Scanner(System.in);
        word=sc.nextLine();
        p=word.length();
        for(i=p-1;i>=0;i--)
        {
            ch=word.charAt(i);
            word1=word1+ch;
        }
        if(word.equals(word1))
        System.out.println("Palindrome");
        else
        System.out.println("Not Palindrome");
        
    }
}
