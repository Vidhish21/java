import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		String st,st1=" ",st2=" ";
		int i,p;
		char chr;
		System.out.println("Enter a string:");
		Scanner sc=new Scanner(System.in);
		st=sc.nextLine();
		st=st+' ';
		p=st.length();
		for(i=0;i<p;i++)
		{
		    chr=st.charAt(i);
		    if(chr==' ')
		    {
		        st2=st2+' '+st1;
		        st1=" ";
		    }
		    else
		    st1=chr+st1;
		}
		System.out.println(st2);
		    }
		}
	


