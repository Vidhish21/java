import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int a, b, c;
	    Scanner sc = new Scanner(System.in);
		System.out.println("Enter a and b : ");
		a=sc.nextInt();
	    b=sc.nextInt();
		System.out.println("Before Swapping\na = "+a+"\nb = "+b);
		c=a;
		a=b;
		b=c;
		System.out.println("After Swapping\na = " +a+"\nb = "+b);
	}
}
